{
  "name": "saad-khawaja-backend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "npx tsc && nodemon Dist/app.js",
    "start": "npx tsc && node Dist/app.js",
    "build": "tsc",
    "test": "jest"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testMatch": [
      "**/?(*.)+(spec|test).ts"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  },
  "author": "Saad Khawaja",
  "license": "ISC",
  "description": "this code is written for node js test",
  "dependencies": {
    "axios": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.3.0",
    "@types/node-cache": "^4.2.5",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
